{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "plugins": [
    "prettier-plugin-organize-imports",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^(@xyflow/(.*)|ag-grid-(community|react)/(.*)).css$",
    "^\\./(.*).css$",
    "^(react(/.*)?)",
    "^@(mui/material/(.*)|toolpad/core/(.*))",
    "^@mui/icons-material/(.*)",
    "^@(xyflow|monaco-editor)/(.*)",
    "^ag-grid-(community|react)(/(.*))?$",
    "^@/lib(/(.*))?",
    "^@/layouts(/(.*))?",
    "^@/components(/(.*))?",
    "^@/views(/(.*))?",
    "^@/router(/(.*))?",
    "^@/App",
    "^\\./(.*)",
    "^\\.\\./(.*)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
